@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix vad: <http://example.org/vad#>.

vad:ptree {
vad:ptree a vad:ObjectTree;
    rdfs:label "Дерево Процессов (TriG)";
    vad:hasParentObj vad:root.
vad:p1 a vad:TypeProcess;
    rdfs:label "Процесс 1 Изготовление скрепки";
    vad:hasParentObj vad:ptree;
    vad:hasTrig vad:t_p1.
vad:p1_1 a vad:TypeProcess;
    rdfs:label "Процесс 1.1 Отрезать проволоку";
    vad:hasParentObj vad:p1.
vad:p1_2 a vad:TypeProcess;
    rdfs:label "Процесс 1.2 Согнуть проволоку";
    vad:hasParentObj vad:p1
}
vad:rtree {
vad:rtree a vad:ObjectTree;
    rdfs:label "Дерево Исполнителей (TriG)";
    vad:hasParentObj vad:root.
vad:Org-structure a vad:TypeExecutor;
    rdfs:label "Исполнитель Org-structure";
    vad:hasParentObj vad:rtree.
vad:Executor1 a vad:TypeExecutor;
    rdfs:label "Исполнитель 1";
    vad:hasParentObj vad:Org-structure.
vad:Executor2 a vad:TypeExecutor;
    rdfs:label "Исполнитель 2";
    vad:hasParentObj vad:Org-structure
}
vad:t_p1 {
vad:t_p1 a vad:VADProcessDia;
    rdfs:label "Схема процесса Процесс 1 Изготовление скрепки";
    vad:hasParentObj vad:p1.
vad:p1_1 vad:isSubprocessTrig vad:t_p1;
    vad:hasExecutor vad:ExecutorGroup_p1_1;
    vad:hasNext vad:p1_2.
vad:p1_2 vad:isSubprocessTrig vad:t_p1;
    vad:hasExecutor vad:ExecutorGroup_p1_2.
vad:ExecutorGroup_p1_1 a vad:ExecutorGroup;
    dcterms:description "Группа исполнителей процесса p1_1";
    vad:includes vad:Executor1.
vad:ExecutorGroup_p1_2 a vad:ExecutorGroup;
    dcterms:description "Группа исполнителей процесса p1_2";
    vad:includes vad:Executor2
}
