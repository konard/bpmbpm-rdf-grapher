vad:t_p1 {
vad:t_p1 a vad:VADProcessDia;
    rdfs:label "Схема t_p1 процесса p1";
    vad:hasParentObj vad:p1.
<http://example.org/vad#p1.1> vad:isSubprocessTrig vad:t_p1;
    vad:hasExecutor <http://example.org/vad#ExecutorGroup_p1.1>;
    vad:hasNext <http://example.org/vad#p1.2>.
<http://example.org/vad#p1.2> vad:isSubprocessTrig vad:t_p1;
    vad:hasExecutor <http://example.org/vad#ExecutorGroup_p1.2>.
<http://example.org/vad#ExecutorGroup_p1.1> a vad:ExecutorGroup;
    rdfs:label "Группа исполнителей процесса p1.1";
    vad:includes vad:Executor1.
<http://example.org/vad#ExecutorGroup_p1.2> a vad:ExecutorGroup;
    rdfs:label "Группа исполнителей процесса p1.2";
    vad:includes vad:Executor1, vad:Executor2
}
<http://example.org/vad#t_p1.1> {
<http://example.org/vad#t_p1.1> a vad:VADProcessDia;
    rdfs:label "Схема t_p1.1 процесса p1.1";
    vad:hasParentObj <http://example.org/vad#p1.1>.
<http://example.org/vad#p1.1.1> vad:isSubprocessTrig <http://example.org/vad#t_p1.1>;
    vad:hasExecutor <http://example.org/vad#ExecutorGroup_p1.1.1>;
    vad:hasNext vad:p1_1_2.
<http://example.org/vad#ExecutorGroup_p1.1.1> a vad:ExecutorGroup;
    rdfs:label "Группа исполнителей процесса p1.1.1";
    vad:includes vad:Executor21.
vad:p1_1_2 vad:isSubprocessTrig <http://example.org/vad#t_p1.1>;
    vad:hasExecutor vad:ExecutorGroup_p1_1_2.
vad:ExecutorGroup_p1_1_2 a vad:ExecutorGroup;
    rdfs:label "Группа исполнителей процесса p1_1_2";
    vad:includes vad:Executor21, vad:Executor22
}
vad:t_p2 {
<http://example.org/vad#p1.1> vad:isSubprocessTrig vad:t_p2;
    vad:hasExecutor vad:ExecutorGroup_p1_1;
    vad:hasNext vad:p1_1_1.
vad:t_p2 a vad:VADProcessDia;
    rdfs:label "Схема t_p2 процесса p2";
    vad:hasParentObj vad:p2.
<http://example.org/vad#pх.х> vad:isSubprocessTrig vad:t_p2;
    vad:hasExecutor <http://example.org/vad#ExecutorGroup_pх.х>.
vad:p2_1 vad:isSubprocessTrig vad:t_p2;
    vad:hasExecutor vad:ExecutorGroup_p2_1;
    vad:hasNext <http://example.org/vad#p1.1>, vad:p2_2.
vad:ExecutorGroup_p2_1 a vad:ExecutorGroup;
    rdfs:label "Группа исполнителей процесса p2_1";
    vad:includes vad:Executor1.
vad:p2_2 vad:isSubprocessTrig vad:t_p2;
    vad:hasExecutor vad:ExecutorGroup_p2_2.
vad:ExecutorGroup_p2_2 a vad:ExecutorGroup;
    rdfs:label "Группа исполнителей процесса p2_2";
    vad:includes vad:Executor1, vad:Executor2.
vad:ExecutorGroup_p1_1 a vad:ExecutorGroup;
    rdfs:label "Группа исполнителей процесса p1_1";
    vad:includes vad:Executor1, vad:Executor2.
vad:p1_1_1 vad:isSubprocessTrig vad:t_p2;
    vad:hasExecutor vad:ExecutorGroup_p1_1_1;
    vad:hasNext <http://example.org/vad#pх.х>.
vad:ExecutorGroup_p1_1_1 a vad:ExecutorGroup;
    rdfs:label "Группа исполнителей процесса p1_1_1";
    vad:includes vad:Executor1, vad:Executor2.
<http://example.org/vad#ExecutorGroup_pх.х> a vad:ExecutorGroup;
    rdfs:label "Группа исполнителей процесса pх.х";
    vad:includes vad:Executor1, vad:Executor2
}
