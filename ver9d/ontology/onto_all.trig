        dcterms:description "Связывает тип с его группой предикатов" .

    vad:contextTriGType
        rdf:type rdf:Property, owl:ObjectProperty ;
        rdfs:label "contextTriGType" ;
        rdfs:domain vad:Tech ;
        rdfs:range rdfs:Class ;
