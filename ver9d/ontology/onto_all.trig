        rdfs:comment "Метод удаления индивида процесса из текущей схемы" ;
        dcterms:description "Удаляет индивид процесса из текущей схемы" .

    # Метод: Delete Individ Executor
    vad:DeleteIndividExecutor
        rdf:type vad:ObjectMethod ;
        rdfs:label "Delete Individ Executor" ;
        vad:methodForType vad:ExecutorGroup ;
        vad:methodFunction "deleteIndividExecutor" ;
        rdfs:comment "Метод удаления исполнителей из группы ExecutorGroup" ;
        dcterms:description "Удаляет всех исполнителей из группы ExecutorGroup" .

    # Метод: Add hasNext Dia
    vad:AddHasNextDia
        rdf:type vad:ObjectMethod ;
        rdfs:label "Add hasNext Dia" ;
        vad:methodForType vad:isSubprocessTrig ;
        vad:methodFunction "addHasNextDia" ;
        rdfs:comment "Метод редактирования vad:hasNext для индивида процесса" ;
        dcterms:description "Редактирование vad:hasNext для индивида процесса" .

    # Метод: Edit Label Concept Process
    vad:EditLabelConceptProcess
        rdf:type vad:ObjectMethod ;
        rdfs:label "Edit Label Concept Process" ;
        vad:methodForType vad:isSubprocessTrig ;
        vad:methodFunction "editLabelConceptProcess" ;
        rdfs:comment "Метод редактирования rdfs:label для концепта процесса" ;
        dcterms:description "Редактирование rdfs:label для концепта процесса и связанной схемы" .

    # ==========================================================================
    # МЕТОДЫ ДИАГРАММЫ
    # ==========================================================================

    vad:DiagramMethod
        rdf:type rdfs:Class, owl:Class ;
        rdfs:subClassOf vad:Tech ;
        rdfs:label "DiagramMethod" ;
        rdfs:comment "Класс DiagramMethod определяет методы (действия), которые можно выполнить над текущей открытой диаграммой (TriG)" ;
        dcterms:description "Класс для определения методов диаграммы" .

    # Метод диаграммы: Del Dia
    vad:DelDia
        rdf:type vad:DiagramMethod ;
        rdfs:label "Del Dia" ;
        vad:methodFunction "delDia" ;
        rdfs:comment "Метод удаления текущей открытой схемы процесса (TriG)" ;
        dcterms:description "Удаление текущей открытой схемы процесса (TriG)" .

}

