        rdfs:comment "Автоматическое вычисление rdfs:label для ExecutorGroup через reasoning" ;
        dcterms:description "Автоматическое вычисление rdfs:label для ExecutorGroup через reasoning" .

    # ==========================================================================
    # МЕТОДЫ ОБЪЕКТОВ
    # ==========================================================================

    vad:ObjectMethod
        rdf:type rdfs:Class, owl:Class ;
        rdfs:subClassOf vad:Tech ;
        rdfs:label "ObjectMethod" ;
        rdfs:comment "Класс ObjectMethod определяет методы (действия), которые можно выполнить над объектами определённого типа на схеме процесса" ;
        dcterms:description "Класс для определения методов объектов на схеме" .

    vad:methodForType
